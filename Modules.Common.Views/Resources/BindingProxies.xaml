<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:commonDd="clr-namespace:Modules.Common.Views.DragDrop"
                    xmlns:services="clr-namespace:Modules.Common.Views.Services">

    <services:UIScalerBindingProxy x:Key="UIScaler" UIScaler="{x:Static services:UIScaler.Instance}" />
    <commonDd:CustomDropHandlerBindingProxy x:Key="CustomDropHandler" Handler="{x:Static commonDd:CustomDropHandler.Instance}" />

</ResourceDictionary>