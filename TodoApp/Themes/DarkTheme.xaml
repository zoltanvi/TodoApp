<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Color x:Key="Transparent">#00000000</Color>
    <Color x:Key="ThemeOpposite">#FFFFFF</Color>
    <Color x:Key="ThemePrimary">black</Color>

    <Color x:Key="TagGrey">#dadce0</Color>
    <Color x:Key="TagBlue">#8ab4f8</Color>
    <Color x:Key="TagRed">#f28b82</Color>
    <Color x:Key="TagYellow">#fdd663</Color>
    <Color x:Key="TagGreen">#81c995</Color>
    <Color x:Key="TagPink">#ff8bcb</Color>
    <Color x:Key="TagPurple">#c58af9</Color>
    <Color x:Key="TagCyan">#78d9ec</Color>
    <Color x:Key="TagOrange">#fcad70</Color>


    <Color x:Key="MessageInfo">#5D9CF6</Color>
    <Color x:Key="MessageInfoBg">#112545</Color>
    <Color x:Key="MessageInfoBorder">#5D9CF6</Color>

    <Color x:Key="MessageWarning">#d89614</Color>
    <Color x:Key="MessageWarningBg">#2b2111</Color>
    <Color x:Key="MessageWarningBorder">#d89614</Color>

    <Color x:Key="MessageError">#dc4446</Color>
    <Color x:Key="MessageErrorBg">#2c1618</Color>
    <Color x:Key="MessageErrorBorder">#dc4446</Color>

    <!--  SolidColorBrushes for colors  -->
    <SolidColorBrush x:Key="TransparentBrush" Color="{DynamicResource Transparent}" />
    <SolidColorBrush x:Key="ThemeOppositeBrush" Color="{DynamicResource ThemeOpposite}" />
    <SolidColorBrush x:Key="ThemePrimaryBrush" Color="{DynamicResource ThemePrimary}" />

